<style>
.project-structure {
  padding: 20px;
  font-family: monospace;
}

.file-tree {
  list-style-type: none;
  padding-left: 20px;
}

.file-tree ul {
  list-style-type: none;
  padding-left: 30px;
}

.folder {
  font-weight: bold;
  margin: 10px 0;
}

.file {
  margin: 5px 0;
  color: #666;
}

.empty-folder {
  color: #999;
  font-style: italic;
  margin-left: 30px;
  font-size: 0.9em;
}
</style>

<script>
document.addEventListener('DOMContentLoaded', function() {
  const folders = document.querySelectorAll('.folder');
  folders.forEach(folder => {
    const ul = folder.querySelector('ul');
    if (ul && !ul.children.length) {
      ul.remove();
      const emptyMsg = document.createElement('span');
      emptyMsg.className = 'empty-folder';
      emptyMsg.textContent = '(empty)';
      folder.appendChild(emptyMsg);
    }
  });
});
</script>